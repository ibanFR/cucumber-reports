{
  "name": "cucumber-reports",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "fix": "eslint --max-warnings 0 features functions src --fix && prettier --write features functions src",
    "lint": "eslint --max-warnings 0 features functions src && prettier --check features functions src",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:acceptance": "node --import @cucumber/node/bootstrap --import tsx --test-concurrency=1 --test \"features/**/*.feature\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.731.1",
    "@aws-sdk/s3-request-presigner": "^3.731.1",
    "@cucumber/messages": "^29.0.0",
    "@cucumber/react-components": "^23.2.0",
    "@sentry/react": "^10.8.0",
    "@tanstack/react-query": "^5.85.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-ga4": "^2.1.0",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@cucumber/node": "^0.4.0",
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^22.15.32",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "puppeteer": "^24.4.0",
    "sass-embedded": "^1.91.0",
    "tailwindcss": "^4.1.12",
    "tsx": "^4.19.3",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.0.9"
  }
}
